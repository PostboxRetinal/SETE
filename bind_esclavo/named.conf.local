// ZONA DIRECTA en ESCLAVO
zone "empresa.local" {
    type slave;
    file "/var/cache/bind/db.empresa.local";
    masters { 192.168.56.2; };
};

// ZONA INVERSA en ESCLAVO
zone "56.168.192.in-addr.arpa" {
    type slave;
    file "/var/cache/bind/db.192";
    masters { 192.168.56.2; };
};
